@using GridMvc.Html
@model FastBookCreator.Models.ItemDetail

@(
    Html.(Model).Named("ItemDetail")
        .Columns(columns =>
        {
            columns.Add(f => f.FirstName).Titled("First Name");
            columns.Add(f => f.LastName).Titled("Last Name");
            columns.Add(f => f.Age).Titled("Age");
        }))